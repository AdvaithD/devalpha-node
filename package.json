{
  "name": "vester",
  "version": "0.1.0",
  "repository": "https://github.com/fhqvst/vester",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.29.0",
    "decimal.js": "^10.0.0",
    "fastpriorityqueue.ts": "^0.1.2",
    "highland": "^3.0.0-beta.6",
    "redux": "^3.7.2",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "@types/highland": "^2.10.8",
    "@types/mathjs": "0.0.39",
    "@types/node": "^9.6.2",
    "@types/socket.io": "^1.4.33",
    "ava": "^1.0.0-beta.4",
    "documentation": "^6.2.0",
    "nyc": "^11.1.0",
    "sinon": "^4.1.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "dev": "NODE_ENV=development tsc --watch",
    "api": "documentation readme dist/**/* --section=API --markdown-toc=false",
    "start": "npm run build",
    "lint": "tslint lib/**/*",
    "test": "npm run build && NODE_ENV=test ava tests/**/*.spec.js",
    "test-only": "NODE_ENV=test ava",
    "coverage": "NODE_ENV=test nyc ava tests/**/*.spec.js"
  },
  "nyc": {
    "exclude": [
      "tests/",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
