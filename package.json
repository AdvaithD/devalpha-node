{
  "name": "vester",
  "version": "0.0.2",
  "repository": "https://github.com/fhqvst/vester",
  "main": "dist/vester.js",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.29.0",
    "decimal.js": "^10.0.0",
    "fastpriorityqueue.ts": "^0.1.2",
    "highland": "^3.0.0-beta.6",
    "lodash": "^4.17.5",
    "mathjs": "^3.17.0",
    "redux": "^3.7.2",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "@types/highland": "^2.10.8",
    "@types/mathjs": "0.0.39",
    "@types/node": "^9.6.2",
    "@types/socket.io": "^1.4.33",
    "ava": "^1.0.0-beta.3",
    "documentation": "^5.3.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "nyc": "^11.1.0",
    "sinon": "^4.1.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "dev": "NODE_ENV=development tsc --watch",
    "docs:api": "documentation lint lib && node scripts/generateApi.js",
    "docs:build": "cd docs && $(npm bin)/hexo generate",
    "docs:watch": "cd docs && $(npm bin)/hexo server",
    "docs:publish": "npm run docs:build && cd docs && $(npm bin)/hexo deploy",
    "start": "npm run build",
    "lint": "eslint lib",
    "test": "NODE_ENV=test ava tests/**/*.spec.js",
    "test-only": "NODE_ENV=test ava",
    "report": "nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc ava tests/**/*.spec.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          [
            "@babel/preset-env"
          ]
        ],
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread"
        ]
      }
    },
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "nyc": {
    "exclude": [
      "tests/"
    ]
  }
}
