{
  "name": "vester",
  "version": "0.0.0",
  "main": "dist/cjs/bundle.js",
  "license": "MIT",
  "dependencies": {
    "CBuffer": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "babyparse": "^0.4.6",
    "fastpriorityqueue": "^0.3.1",
    "highland": "^2.11.1",
    "immutable": "^3.8.1",
    "mathjs": "^3.16.2",
    "moment": "^2.18.1",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "documentation": "^5.3.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "handlebars": "^4.0.11",
    "http-server": "^0.10.0",
    "metalsmith": "^2.3.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-headings": "^0.2.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-sass": "^1.4.0",
    "metalsmith-uglify": "^2.0.1",
    "metalsmith-watch": "^1.0.3",
    "nyc": "^11.1.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^3.2.1",
    "spectre.css": "^0.4.5"
  },
  "scripts": {
    "build:es": "NODE_ENV=production babel lib -d dist/es",
    "build:cjs": "NODE_ENV=production rollup --config",
    "build": "NODE_ENV=production yarn build:es & yarn build:cjs",
    "docs:api": "node scripts/generateApi.js",
    "docs:build": "rm -rf .docs && node scripts/buildDocs.js",
    "docs:watch": "http-server .docs --cors & node scripts/buildDocs.js --watch",
    "docs:publish": "NODE_ENV=production yarn docs:build && cd .docs && git init && git commit --allow-empty -m 'Updated docs' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'Updated docs' && git push git@github.com:fhqvst/vester gh-pages --force",
    "start": "babel-node lib/index.js",
    "test": "NODE_ENV=test ava tests/**/*.spec.js",
    "test-only": "NODE_ENV=test ava",
    "report": "nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc ava tests/**/*.spec.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ],
            "node": "8"
          },
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-async-to-generator",
      [
        "transform-object-rest-spread",
        {
          "useBuildIns": true
        }
      ]
    ],
    "ignore": [
      "*.spec.js"
    ],
    "env": {
      "test": {
        "presets": [
          "@ava/stage-4"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "tests/"
    ]
  }
}
