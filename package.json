{
  "name": "vester",
  "version": "0.0.2",
  "repository": "https://github.com/fhqvst/vester",
  "main": "build/umd/vester.js",
  "module": "build/es/index.js",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^1.1.1",
    "fastpriorityqueue": "^0.3.1",
    "highland": "^3.0.0-beta.6",
    "immutable": "^3.8.1",
    "mathjs": "^3.17.0",
    "moment": "^2.19.3",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "ava": "^1.0.0-beta.3",
    "documentation": "^5.3.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "nyc": "^11.1.0",
    "sinon": "^4.1.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development WEBPACK_ENV=development webpack --progress --colors --watch",
    "build": "NODE_ENV=production babel lib -d build/",
    "docs:api": "documentation lint lib && node scripts/generateApi.js",
    "docs:build": "cd docs && $(npm bin)/hexo generate",
    "docs:watch": "cd docs && $(npm bin)/hexo server",
    "docs:publish": "npm run docs:build && cd docs && $(npm bin)/hexo deploy",
    "start": "npm run build",
    "lint": "eslint lib",
    "test": "NODE_ENV=test ava tests/**/*.spec.js",
    "test-only": "NODE_ENV=test ava",
    "report": "nyc report --reporter=html",
    "coverage": "NODE_ENV=test nyc ava tests/**/*.spec.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          [
            "@babel/preset-env"
          ]
        ],
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread"
        ]
      }
    },
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "nyc": {
    "exclude": [
      "tests/"
    ]
  }
}
